	<!-- sticky starts-->
	<script>
	   jQuery(document).ready(function() {
		   jQuery(window).scroll(function() {
			   if (jQuery(this).scrollTop() > 350) {
				   jQuery('.stickypart').addClass("stick");

			   } else {
				   jQuery('.stickypart').removeClass("stick");

			   }
		   });
	   });
	</script>
